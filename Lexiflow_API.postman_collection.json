				{
					"name": "Sign In",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const jsonData = pm.response.json();",
									"    if (jsonData.data && jsonData.data.tokens) {",
									"        pm.environment.set('accessToken', jsonData.data.tokens.access.token);",
									"        pm.environment.set('refreshToken', jsonData.data.tokens.refresh.token);",
									"    }",
									"}"
								]
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"identifier\": \"john.doe@example.com\",\n  \"password\": \"SecurePass123!\"\n}"
						},
